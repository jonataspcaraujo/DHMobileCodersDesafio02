<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dhmobilecodersdesafio02">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DHMobileCodersDesafio02">
        <activity android:name=".view.activity.DetalheScreenActivity">
        </activity>
        <activity android:name=".view.activity.PratoScreenActivity">
        </activity>
        <activity android:name=".view.activity.RestauranteScreenActivity">
        </activity>
        <activity android:name=".view.activity.RegisterScreenActivity" />
        <activity android:name=".view.activity.LoginScreenActivity" />
        <activity android:name=".view.activity.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".view.MainActivity" />
    </application>

</manifest>